#include "cute.h"
#include "demo.h"

int plus(int lhs, int rhs) {
	return lhs + rhs;
}

int minus(int lhs, int rhs) {
	return lhs - rhs;
}

void testZeroPlusZero() {
	ASSERT_EQUAL(0, plus(0, 0));
}

void testSevenPlusThree() {
	ASSERT_EQUAL(10, plus(7, 3));
}

cute::suite make_suite_demo() {
	cute::suite s;
	s.push_back(CUTE(testZeroPlusZero));
	return s;
}
